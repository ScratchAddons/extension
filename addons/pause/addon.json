{
  "name": "Pause button",
  "description": "Adds a pause button next to the green flag.",
  "info": [
    {
      "type": "notice",
      "text": "To programmatically pause a project in Scratch (also known as breakpoints), use the \"breakpoint\" block provided by the \"debugger\" addon.",
      "id": "programpause"
    }
  ],
  "credits": [
    {
      "name": "Jeffalo"
    },
    {
      "name": "GarboMuffin"
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "versionAdded": "1.3.0",
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "userstyles": [
    {
      "url": "style.css",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "tags": ["editor", "projectPlayer", "recommended"],
  "enabledByDefault": true
}
